{
  "name": "fluss-admin",
  "main": "src/pages/index",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "sass-types": "./node_modules/.bin/tsm src --nameFormat none --exportType default",
    "sass-types:watch": "./node_modules/.bin/tsm src -w --nameFormat none --exportType default",
    "css-types": "./node_modules/.bin/tcm src",
    "css-types:watch": "./node_modules/.bin/tcm src -w"
  },
  "dependencies": {
    "@fontsource/nunito": "^4.2.1",
    "axios": "^0.21.1",
    "mobx": "^6.1.7",
    "mobx-react": "^7.1.0",
    "next": "10.0.7",
    "react-hook-form": "^6.15.5",
    "react-input-mask": "^2.0.4",
    "react-map-gl": "^6.1.10",
    "sass": "^1.32.8",
    "shared": "file:../shared"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "ts-node": "^9.1.1",
    "typed-css-modules": "^0.6.5",
    "typed-scss-modules": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "bit": {
    "env": {
      "compiler": {
        "bit.envs/compilers/react-typescript@4.0.0": {
          "rawConfig": {
            "tsconfig": {
              "compilerOptions": {
                "module": "CommonJS"
              }
            }
          }
        }
      }
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm",
    "resolveModules": {
      "modulesDirectories": [
        "src"
      ]
    },
    "overrides": {
      "*": {
        "devDependencies": {
          "@types/node": "^14.14.31",
          "@types/react": "^17.0.2"
        },
        "peerDependencies": {
          "react": "17.0.1"
        }
      }
    }
  }
}
